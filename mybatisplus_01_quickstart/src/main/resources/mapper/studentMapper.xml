<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yongren.github.mapper.StudentMapper">
    <select id="queryAll" resultType="com.yongren.github.domain.Student">
        select * from student;
    </select>

    <select id="findById" resultType="com.yongren.github.domain.Student">
        select * from student where id = #{id}
    </select>
    
    <select id="queryAllFunction" resultType="com.yongren.github.domain.VehicleFuction">
        select * from vehicle_funtion;
    </select>
</mapper>